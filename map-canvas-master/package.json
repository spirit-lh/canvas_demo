{
  "name": "map-canvas",
  "version": "1.0.0",
  "description": "基于baidu地图、canvas数据可视化",
  "main": "rollup.config.js",
  "scripts": {
    "mini": "npm run build && uglifyjs build/release/baidu-map-typhoon.js -c -m -o build/release/baidu-map-typhoon.min.js",
    "build": "rollup -c",
    "watch": "npm-watch",
    "test": "npm run build"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "main.js"
      ],
      "extensions": "js,jsx",
      "ignore": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chengquan223/map-canvas.git"
  },
  "keywords": [
    "baidu",
    "arcgis",
    "amap",
    "google",
    "canvas",
    "visualization"
  ],
  "author": "309581454@qq.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chengquan223/map-canvas/issues"
  },
  "homepage": "https://github.com/chengquan223/map-canvas#readme",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^3.0.0",
    "npm-watch": "^0.1.8",
    "rollup": "^0.41.5",
    "rollup-plugin-babel": "^2.7.1",
    "uglify-js": "^2.8.12"
  }
}